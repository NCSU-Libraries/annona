{
  "name": "iiif-annotation",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && ./docs.sh",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "core-js": "^3.0.1",
    "document-register-element": "^1.13.1",
    "flush-promises": "^1.0.2",
    "jest-canvas-mock": "^1.1.0",
    "node-sass": "^4.11.0",
    "openseadragon": "^2.4.0",
    "sass-loader": "^7.1.0",
    "truncate-html": "^1.0.1",
    "vue": "^2.5.21",
    "vue-custom-element": "^3.2.6",
    "vue-fullscreen": "^2.1.3",
    "webpack": "^4.29.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.1",
    "@vue/cli-service": "^3.2.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-jest": "^23.6.0",
    "eslint": "^5.10.0",
    "jest": "^23.6.0",
    "vue-jest": "^3.0.2",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "setupFiles": [
      "jest-canvas-mock",
      "core-js",
      "<rootDir>/test/__mocks__/canvas.js"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
